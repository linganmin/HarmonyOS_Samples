{
  "apiType": "stageMode",
  // [Start call_custom_method_json]
  "buildOption": {
    "externalNativeOptions": {
      "path": "./src/main/cpp/CMakeLists.txt",
      "arguments": "",
      "cppFlags": "",
    },
    "arkOptions": {
      "runtimeOnly": {
        "sources": [
          "./src/main/ets/pages/Test.ets"
        ]
      }
    },
  },
  // [End call_custom_method_json]
  "buildOptionSet": [
    {
      "name": "release",
