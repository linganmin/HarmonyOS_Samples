{
  "name": "MyReact",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pack:pkg": "cd ../react-native-huawei-authentication && npm install && npm pack && cd ../MyReact",
    "install:pkg": "npm run pack:pkg && npm i react-native-huawei-authentication@file:../react-native-huawei-authentication/react-native-huawei-authentication-0.0.1.tgz",
    "dev": "npm uninstall react-native-huawei-authentication && npm run install:pkg && npm run codegen && react-native bundle-harmony --dev",
    "release": "npm uninstall react-native-huawei-authentication && npm run install:pkg && npm run codegen && react-native bundle-harmony --dev=false --minify=true",
    "codegen": "react-native codegen-harmony --rnoh-module-path ./harmony/oh_modules/@rnoh/react-native-openharmony"
  },
  "dependencies": {
    "@formatjs/intl-getcanonicallocales": "2.5.5",
    "@react-native-oh-tpl/react-native-gesture-handler": "2.14.16",
    "@react-native-oh-tpl/react-native-localize": "3.1.0-0.0.1",
    "@react-native-oh-tpl/react-native-safe-area-context": "4.7.4-0.1.4",
    "@react-native-oh-tpl/react-native-webview": "13.10.2-0.2.27",
    "@react-native-oh/react-native-harmony": "0.72.59",
    "@react-native-ohos/checkbox": "0.5.17-rc.4",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1",
    "i18next": "23.2.3",
    "intl-pluralrules": "2.0.1",
    "react": "18.2.0",
    "react-i18next": "14.0.0",
    "react-native": "0.72.5",
    "react-native-screens": "3.37.0"
  },
  "devDependencies": {
    "@react-native/metro-config": "0.72.12",
    "@types/react": "18.3.21",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
