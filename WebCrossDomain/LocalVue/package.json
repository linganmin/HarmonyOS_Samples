{
  "name": "localvue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:LocalResource": "VUE_APP_TARGET_PAGE=LocalResource vue-cli-service build",
    "build:CustomProtocol": "VUE_APP_TARGET_PAGE=CustomProtocol vue-cli-service build",
    "build:RemoteRequest": "VUE_APP_TARGET_PAGE=RemoteRequest vue-cli-service build",
    "build:CookiesSettingsA": "VUE_APP_TARGET_PAGE=CookiesSettingsA vue-cli-service build",
    "build:CookiesSettingsB": "VUE_APP_TARGET_PAGE=CookiesSettingsB vue-cli-service build",
    "build:FatalPage": "VUE_APP_TARGET_PAGE=FatalPage vue-cli-service build",
    "build:all": "npm run build:LocalResource && npm run build:CustomProtocol && npm run build:RemoteRequest && npm run build:CookiesSettingsA && npm run build:CookiesSettingsB && npm run build:FatalPage"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "core-js": "^3.8.3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
